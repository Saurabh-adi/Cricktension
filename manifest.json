{
    "manifest_version": 3,
    "name": "Cricketension",
    "version": "0.0.1",

    
    "background":{
        "service_worker":"service-worker.js",
        "type":"module"
    },
    "description": "my extension for live cricket scores",
    "author": "AdityaSaurabh", 
    "icons": {
        "16": "icon.png",
        "32": "icon.png" ,
        "48": "icon.png",
        "128": "icon.png"
    },

    
    "action": {
        "default_popup": "real.html",
        "default_icon": {
            "16": "icon.png",
            "32": "icon.png" ,
            "48": "icon.png",
            "128": "icon.png"
        },
        "default_title": "Live Cricket scores"

    },
    "permissions":[
        "scripting",
        "storage",
        "alarms",
        "notifications"

    ],
    "host_permissions":[
        "https://*/*",
        "https://*/*",
        "https://cricapi.com/"
    ]
      
  }
